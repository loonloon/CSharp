<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <SolidColorBrush x:Key="DarkColorBorderBrush" Color="#FF6F8698"/>
    <SolidColorBrush x:Key="LightColorBorderBrush" Color="#FFA0B0BF"/>
    <SolidColorBrush x:Key="DisabledTextBrush" Color="#FFA0B0BF"/>
    <SolidColorBrush x:Key="EnabledTextBrush" Color="#FF2D4650"/>
    <SolidColorBrush x:Key="TitleBackgroundBrush" Color="#FFBEC8D2"/>
    <SolidColorBrush x:Key="ContentBackgroundBrush" Color="#FFF1F2F4"/>
    <SolidColorBrush x:Key="MainBackgroundBrush" Color="#FFDFE4E8"/>
    <LinearGradientBrush x:Key="ButtonBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="White" />
        <GradientStop Offset="1" Color="#DFE4E8"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="MouseOverBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="White" />
        <GradientStop Offset="1" Color="#FF99CCFF"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="PressedBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#FFA0B0BF" />
        <GradientStop Offset="1" Color="#FF99CCFF"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="CheckedBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#FFA0B0BF" />
        <GradientStop Offset="1" Color="#FFDFE4E8"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ProminentButtonBackgroundBrush" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#FFDDEEFF" />
        <GradientStop Offset="1" Color="#FFAACCEE"/>
    </LinearGradientBrush>

    <Style TargetType="Grid">
        <Setter Property="Background" Value="#FFDFE4E8"/>
    </Style>

    <Style TargetType="ListBox">
        <Setter Property="Background" Value="#FFDFE4E8"/>
    </Style>

    <Style TargetType="ToolTip">
        <Setter Property="Placement" Value="Top"/>
        <Setter Property="Background" Value="LightYellow"/>
        <Setter Property="BorderBrush" Value="{StaticResource LightColorBorderBrush}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToolTip">
                    <Viewbox>
                        <Border Background="LightYellow" BorderBrush="{StaticResource LightColorBorderBrush}" Padding="3">
                            <TextBlock Text="{TemplateBinding Content}" TextWrapping="Wrap" TextAlignment="Justify" />
                        </Border>
                    </Viewbox>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <Trigger Property="Content" Value="{x:Static sys:String.Empty}">
                <Setter Property="Visibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="Content" Value="{x:Null}">
                <Setter Property="Visibility" Value="Collapsed" />
            </Trigger>
        </Style.Triggers>

    </Style>

    <Style x:Key="ContentsBackground" TargetType="{x:Type Grid}">
        <Setter Property="Background" Value="#FFF1F2F4" />
    </Style>

    <Style TargetType="TextBlock" x:Key="FMTextBlockBase">
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="FontFamily" Value="Segoe UI"/>
        <Setter Property="Foreground" Value="{StaticResource EnabledTextBrush}"/>
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Foreground" Value="{StaticResource DisabledTextBrush}"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="TextBlock" BasedOn="{StaticResource FMTextBlockBase}"/>

    <Style TargetType="TextBox" x:Key="FMTextBoxBase">
        <Setter Property="Height" Value="40"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="ScrollViewer.Background" Value="White"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Foreground" Value="{StaticResource EnabledTextBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource DarkColorBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1.1"/>
        <Setter Property="SelectionBrush" Value="#FFBBDDFF"/>
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Foreground" Value="{StaticResource DisabledTextBrush}"/>
                <Setter Property="BorderBrush" Value="#FFBEC8D2"/>
            </Trigger>
            <Trigger Property="IsReadOnly" Value="True">
                <Setter Property="Background" Value="{StaticResource ContentBackgroundBrush}"/>
                <Setter Property="BorderBrush" Value="#FFBEC8D2"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="BorderBrush" Value="#FF77BBFF"/>
            </Trigger>
            <Trigger Property="IsKeyboardFocused" Value="True">
                <Setter Property="BorderBrush" Value="#FF3399FF"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="TextBox" BasedOn="{StaticResource FMTextBoxBase}"></Style>

    <Style TargetType="TextBox" x:Key="FreeStyleTextBox">
        <Setter Property="VerticalAlignment" Value="Stretch"/>
        <Setter Property="HorizontalAlignment" Value="Stretch"/>
        <Setter Property="ScrollViewer.Background" Value="White"/>
        <Setter Property="BorderBrush" Value="{StaticResource DarkColorBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1.1"/>
        <Setter Property="Foreground" Value="{StaticResource EnabledTextBrush}"/>
        <Setter Property="SelectionBrush" Value="#FFBBDDFF"/>
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Foreground" Value="{StaticResource DisabledTextBrush}"/>
                <Setter Property="BorderBrush" Value="#FFBEC8D2"/>
            </Trigger>
            <Trigger Property="IsReadOnly" Value="True">
                <Setter Property="Background" Value="{StaticResource ContentBackgroundBrush}"/>
                <Setter Property="BorderBrush" Value="#FFBEC8D2"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="BorderBrush" Value="#FF77BBFF"/>
            </Trigger>
            <Trigger Property="IsKeyboardFocused" Value="True">
                <Setter Property="BorderBrush" Value="#FF3399FF"/>
            </Trigger>
        </Style.Triggers>
    </Style>


    <Style TargetType="PasswordBox" x:Key="FMPasswordBoxBase">
        <Setter Property="ScrollViewer.Background" Value="White"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Foreground" Value="{StaticResource EnabledTextBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource DarkColorBorderBrush}"/>
        <Setter Property="BorderThickness" Value="1.1"/>
        <Setter Property="SelectionBrush" Value="#FFBBDDFF"/>
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Foreground" Value="{StaticResource DisabledTextBrush}"/>
                <Setter Property="BorderBrush" Value="#FFBEC8D2"/>
            </Trigger>
            <Trigger Property="IsKeyboardFocused" Value="True">
                <Setter Property="BorderBrush" Value="#FF3399FF"/>
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="BorderBrush" Value="#FF77BBFF"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="PasswordBox" BasedOn="{StaticResource FMPasswordBoxBase}"/>

    <Style TargetType="ComboBox" x:Key="FMComboBoxBase">
        <Style.Resources>
            <Style TargetType="TextBox" BasedOn="{StaticResource FMTextBoxBase}">
            </Style>
        </Style.Resources>
        <Setter Property="Height" Value="40"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="Background" Value="{StaticResource ButtonBackgroundBrush}"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Vertical" Background="{StaticResource MainBackgroundBrush}"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="ComboBox" BasedOn="{StaticResource FMComboBoxBase}">
    </Style>

    <Style TargetType="MenuItem">
        <Setter Property="FontFamily" Value="Meiryo"/>
        <Style.Resources>
            <Style TargetType="Grid">
                <Setter Property="Background" Value="Transparent"/>
            </Style>
            <Style TargetType="TextBlock">
                <Setter Property="FontFamily" Value="Meiryo"/>
                <Setter Property="FontSize" Value="14"/>
            </Style>
        </Style.Resources>
    </Style>

    <Style TargetType="{x:Type DataGridColumnHeader}">
        <Setter Property="Foreground" Value="{StaticResource EnabledTextBrush}"/>
        <Setter Property="BorderThickness" Value="0,0,1,0"/>
        <Setter Property="Height" Value="35"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="TextBlock.Margin" Value="0,3,0,3"/>
        <Setter Property="TextBlock.TextTrimming" Value="CharacterEllipsis"/>
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <ContentControl Content="{TemplateBinding Property=Content}" Background="Transparent"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
        <Style.Resources>
            <Style TargetType="ContentPresenter">
                <Setter Property="Margin" Value="8,0,0,0"/>
            </Style>
        </Style.Resources>
    </Style>

    <Style x:Key="DataGridCommon" TargetType="{x:Type DataGrid}">
        <Style.Resources>
            <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#BBDDFF"  />
            <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="#d2dae1"/>
        </Style.Resources>
        <Setter Property="Background" Value="{StaticResource ContentBackgroundBrush}"/>
        <Setter Property="Grid.Background" Value="{StaticResource ContentBackgroundBrush}"/>
        <Setter Property="AlternatingRowBackground" Value="{StaticResource ContentBackgroundBrush}"/>
        <Setter Property="RowBackground" Value="White"/>
        <Setter Property="Foreground" Value="{StaticResource EnabledTextBrush}"/>
        <Setter Property="RowHeaderWidth" Value="0"/>
        <Setter Property="GridLinesVisibility" Value="None"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="IsReadOnly" Value="True"/>
        <Setter Property="AutoGenerateColumns" Value="False"/>
        <Setter Property="CanUserAddRows" Value="False"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="TextBlock.LineHeight" Value="25"/>
        <Setter Property="ScrollViewer.Background" Value="{StaticResource ContentBackgroundBrush}"/>
        <Setter Property="FontFamily" Value="Meiryo"/>

        <Setter Property="EnableRowVirtualization" Value="True"/>
        <Setter Property="VirtualizingPanel.IsVirtualizing" Value="True"/>
        <Setter Property="VirtualizingPanel.VirtualizationMode" Value="Recycling"/>
        <Setter Property="CellStyle">
            <Setter.Value>
                <Style TargetType="DataGridCell">
                    <Setter Property="BorderBrush" Value="Transparent"/>
                    <Setter Property="BorderThickness" Value="0"/>
                    <Setter Property="Foreground" Value="{StaticResource EnabledTextBrush}"/>
                    <Setter Property="VerticalContentAlignment" Value="Center"/>
                    <Style.Triggers>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="{StaticResource DisabledTextBrush}"/>
                        </Trigger>
                    </Style.Triggers>
                </Style>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="DataGrid" BasedOn="{StaticResource DataGridCommon}">
    </Style>

    <Style TargetType="ProgressBar">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ProgressBar}">
                    <Grid>
                        <Border Name="PART_Track" CornerRadius="1" Background="{StaticResource ContentBackgroundBrush}" BorderThickness="1" BorderBrush="{StaticResource DarkColorBorderBrush}"></Border>
                        <Grid Name="PART_Indicator" HorizontalAlignment="Left" Margin="1,2">
                            <Rectangle x:Name="prgBackground" Margin="0" RadiusX="1" RadiusY="1" Fill="#FF00BFFF" ></Rectangle>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <LinearGradientBrush x:Key="OnlineDevicePanelBackground" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="Black" Offset="0"/>
        <GradientStop Color="#FF649DF0" Offset="1"/>
        <GradientStop Color="#FF1B2B42"/>
        <GradientStop Color="#FF304C74"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="SelectControlColor" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="Blue" />
        <GradientStop Offset="0.5" Color="Blue"/>
        <GradientStop Offset="1" Color="#AA3344DD" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="SelectControlColorMouseOver" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="Blue" />
        <GradientStop Offset="1" Color="#FF33FFFF" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="SelectControlColorMousePressed" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#CCFFFFFF" />
        <GradientStop Offset="0.1" Color="#CCFFFFFF" />
        <GradientStop Offset="0.4" Color="#88FFFFFF" />
        <GradientStop Offset="0.4" Color="#00FFFFFF" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GradationOrange" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="Crimson" />
        <GradientStop Offset="1" Color="Orange" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GradationGreen" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="Green" />
        <GradientStop Offset="0.5" Color="Green" />
        <GradientStop Offset="1" Color="#AA00FF00" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GradationYellowGreen" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="GreenYellow" />
        <GradientStop Offset="1" Color="Yellow" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GradationRed" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="Red" />
        <GradientStop Offset="1" Color="#AAFF0000" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GradationOrangeYellow" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="Orange" />
        <GradientStop Offset="1" Color="Yellow" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="UserRegistedColor" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#FF00AAFF" />
        <GradientStop Offset="0.5" Color="#FF00AAFF"/>
        <GradientStop Offset="1" Color="#AA44CCFF" />
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="GlossWhiteTop" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#CCFFFFFF" />
        <GradientStop Offset="0.1" Color="#CCFFFFFF" />
        <GradientStop Offset="0.4" Color="#88FFFFFF" />
        <GradientStop Offset="0.4" Color="#00FFFFFF" />
    </LinearGradientBrush>
    <RadialGradientBrush x:Key="GlossWhiteBottom" GradientOrigin="0.5,1.0" Center="0.5,1.0" RadiusX="0.7" RadiusY="0.1">
        <RadialGradientBrush.GradientStops>
            <GradientStop Color="#AAFFFFFF" Offset="0" />
            <GradientStop Color="#00FFFFFF" Offset="1" />
        </RadialGradientBrush.GradientStops>
    </RadialGradientBrush>


    <Style x:Key="FMButtonBase" TargetType="{x:Type Button}">
        <Setter Property="Height" Value="40"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid Background="{x:Null}">
                        <Border Name="Border" CornerRadius="2" Background="{TemplateBinding Property=Background}" BorderBrush="{TemplateBinding Property=BorderBrush }" BorderThickness="1"/>
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center">
                            <ContentPresenter.Resources>
                                <DataTemplate DataType="UIElement">
                                    <ContentControl Content="{TemplateBinding Content}"/>
                                </DataTemplate>
                            </ContentPresenter.Resources>
                        </ContentPresenter>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="Button" x:Key="FMButton" BasedOn="{StaticResource FMButtonBase}">
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="Background" Value="{StaticResource ButtonBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource DarkColorBorderBrush}"/>
        <Style.Triggers>
            <Trigger Property="IsPressed" Value="true">
                <Setter Property="Background" Value="{StaticResource PressedBackgroundBrush}" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{StaticResource MouseOverBackgroundBrush}" />
            </Trigger>
            <Trigger Property="IsMouseCaptured" Value="True">
                <Setter Property="Background" Value="{StaticResource PressedBackgroundBrush}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="false">
                <Setter Property="Background" Value="{StaticResource MainBackgroundBrush}" />
                <Setter Property="BorderBrush" Value="{StaticResource LightColorBorderBrush}"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="Button" x:Key="TitleBarFunctionButton" BasedOn="{StaticResource FMButton}">
        <Setter Property="Background" Value="{StaticResource TitleBackgroundBrush}"/>
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="false">
                <Setter Property="Background" Value="{StaticResource TitleBackgroundBrush}" />
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style TargetType="Button" x:Key="NoColorButton" BasedOn="{StaticResource FMButton}">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="false">
                <Setter Property="BorderBrush" Value="Transparent"/>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style TargetType="Button" BasedOn="{StaticResource FMButton}">
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="Background" Value="{StaticResource ButtonBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{StaticResource DarkColorBorderBrush}"/>
    </Style>

    <Style x:Key="FMToggleButtonBase" TargetType="{x:Type ToggleButton}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ToggleButton">
                    <Grid Background="{x:Null}">
                        <Border Name="Border" CornerRadius="2" Background="{TemplateBinding Property=Background}" BorderBrush="{TemplateBinding Property=BorderBrush}" BorderThickness="1"/>
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center">
                            <ContentPresenter.Resources>
                                <DataTemplate DataType="UIElement">
                                    <ContentControl Content="{TemplateBinding Content}"/>
                                </DataTemplate>
                            </ContentPresenter.Resources>
                        </ContentPresenter>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="FMToggleButton" TargetType="{x:Type ToggleButton}" BasedOn="{StaticResource FMToggleButtonBase}">
        <Setter Property="Background" Value="{StaticResource ButtonBackgroundBrush}" />
        <Setter Property="Foreground" Value="{StaticResource EnabledTextBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="BorderBrush" Value="{StaticResource DarkColorBorderBrush}"/>
        <Setter Property="FontSize" Value="14"/>
        <Style.Resources>
            <Style TargetType="TextBlock" BasedOn="{StaticResource FMTextBlockBase}">
                <Setter Property="TextWrapping" Value="Wrap"/>
            </Style>
        </Style.Resources>
        <Style.Triggers>
            <Trigger Property="IsChecked" Value="True">
                <Setter Property="Background" Value="{StaticResource CheckedBackgroundBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="true">
                <Setter Property="Background" Value="{StaticResource PressedBackgroundBrush}" />
            </Trigger>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{StaticResource MouseOverBackgroundBrush}" />
            </Trigger>
            <Trigger Property="IsMouseCaptured" Value="True">
                <Setter Property="Background" Value="{StaticResource PressedBackgroundBrush}" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="false">
                <Setter Property="Background" Value="{StaticResource MainBackgroundBrush}" />
                <Setter Property="BorderBrush" Value="{StaticResource LightColorBorderBrush}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style TargetType="ToggleButton" BasedOn="{StaticResource FMToggleButton}"/>


    <Style x:Key="OKButton" TargetType="{x:Type Button}" BasedOn="{StaticResource FMButton}">
        <Setter Property="Width" Value="100"/>
    </Style>

    <Style x:Key="CancelButton" TargetType="{x:Type Button}" BasedOn="{StaticResource FMButton}">
        <Setter Property="Width" Value="100"/>
    </Style>

    <Style x:Key="SelectButton" TargetType="{x:Type Button}" BasedOn="{StaticResource FMButton}">

    </Style>

    <Style x:Key="MenuTriangle" TargetType="Control">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Background="Transparent" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                        <Polygon  Fill="{StaticResource EnabledTextBrush}" HorizontalAlignment="Center" VerticalAlignment="Center"
                              Points="0,5 10,5 5,10 0,5" Name="Triangle"
                              Stroke="{StaticResource EnabledTextBrush}">
                        </Polygon>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter TargetName="Triangle" Property="Fill" Value="{StaticResource DisabledTextBrush}"/>
                            <Setter TargetName="Triangle" Property="Stroke" Value="{StaticResource DisabledTextBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="GridMenuButton" TargetType="{x:Type Button}" BasedOn="{StaticResource FMButton}">
        <Setter Property="Height" Value="34"/>
        <Setter Property="Width" Value="34"/>
        <Setter Property="FontSize" Value="18"/>
        <Setter Property="Background" Value="{StaticResource ButtonBackgroundBrush}"/>
    </Style>

    <Style x:Key="MenuButtonStyle" TargetType="MenuItem" >
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Background="{x:Null}">
                        <Border Name="Border" CornerRadius="2" Background="{StaticResource ButtonBackgroundBrush}" BorderBrush="{StaticResource DarkColorBorderBrush}" BorderThickness="1"/>
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center">
                            <ContentPresenter.Resources>
                                <DataTemplate DataType="UIElement">
                                    <Image Source="Image/MenuTriangle.png" Width="10" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,5,0,0"/>
                                </DataTemplate>
                            </ContentPresenter.Resources>
                        </ContentPresenter>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Background" Value="{StaticResource MouseOverBackgroundBrush}" />
                        </Trigger>
                        <Trigger Property="IsMouseCaptured" Value="True">
                            <Setter Property="Background" Value="{StaticResource PressedBackgroundBrush}" />
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="false">
                            <Setter Property="Background" Value="{StaticResource MainBackgroundBrush}" />
                            <Setter Property="BorderBrush" Value="{StaticResource LightColorBorderBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>



    <Style TargetType="ScrollBar">
        <Setter Property="Background" Value="{StaticResource ContentBackgroundBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
    </Style>

    <Style x:Key="LargeCB" TargetType="CheckBox">
        <Setter Property="LayoutTransform">
            <Setter.Value>
                <ScaleTransform ScaleX="1.8" ScaleY="1.8" />
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="LargeRB" TargetType="RadioButton">
        <Setter Property="LayoutTransform">
            <Setter.Value>
                <ScaleTransform ScaleX="1.4" ScaleY="1.4" />
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="BtnTriggerBold" TargetType="Button">
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="true">
                <Trigger.Setters>
                    <Setter Property="FontWeight" Value="ExtraBlack"/>
                </Trigger.Setters>
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="MenuButton" TargetType="Button">
        <Setter Property="Height"  Value="70"/>
        <Setter Property="Width"  Value="130"/>
    </Style>

    <Style x:Key="OperationButton" TargetType="Button">
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="Height"  Value="55"/>
        <Setter Property="Width"  Value="120"/>
        <Setter Property="VerticalAlignment"  Value="Top"/>
        <Setter Property="HorizontalAlignment"  Value="Left"/>
        <Setter Property="Background" Value="{StaticResource SelectControlColor}" />
    </Style>

    <Style x:Key="Button_40" TargetType="Button">
        <Setter Property="Height"  Value="40"/>
        <Setter Property="Width"  Value="40"/>
    </Style>

    <Style x:Key="Button_60" TargetType="Button">
        <Setter Property="Height"  Value="60"/>
        <Setter Property="Width"  Value="60"/>
    </Style>

    <Style x:Key="ComboBox_CommonRO" TargetType="ComboBox" BasedOn="{StaticResource FMComboBoxBase}">
        <Setter Property="IsEditable" Value="False"/>
    </Style>

    <Style x:Key="TextBox_History" TargetType="TextBox">
        <Setter Property="IsReadOnly" Value="True" />
        <Setter Property="Background" Value="{x:Null}" />
        <Setter Property="BorderBrush" Value="{x:Null}" />
        <Setter Property="BorderThickness" Value="0" />
        <Setter Property="TextWrapping" Value="Wrap" />
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="Foreground" Value="{StaticResource EnabledTextBrush}" />
    </Style>

    <Style x:Key="SplitterButtonIconRight" TargetType="Control">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate>
                    <Grid Background="{TemplateBinding Background}" Width="30" Height="30">
                        <Rectangle Fill="{StaticResource EnabledTextBrush}" Height="5" Width="20" RadiusX="2" RadiusY="2">
                            <Rectangle.RenderTransform>
                                <RotateTransform Angle="45" CenterX="18" CenterY="2">
                                </RotateTransform>
                            </Rectangle.RenderTransform>
                        </Rectangle>
                        <Rectangle Fill="{StaticResource EnabledTextBrush}" Height="5" Width="20" RadiusX="2" RadiusY="2">
                            <Rectangle.RenderTransform>
                                <RotateTransform Angle="-45" CenterX="18" CenterY="2">
                                </RotateTransform>
                            </Rectangle.RenderTransform>
                        </Rectangle>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="SplitterButtonIconLeft" TargetType="Control">
